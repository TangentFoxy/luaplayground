--function
--autorun

local args = {...}
local name
if #args > 1 then
  name = table.remove(args, 1)
  if #name < 1 then name = nil end
end
local str = table.remove(args, 1)
loadstring(str)()
local t = { str = str, name = name, type = "autorun" }
table.insert(_G, t)
if name then
  _G[name] = t
end
return t
